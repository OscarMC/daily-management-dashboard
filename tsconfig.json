{
 "compilerOptions": {
  "target": "ESNext",
  "useDefineForClassFields": true,
  "types": [
   "node",
   "vite/client"
  ],
  "lib": [
   "DOM",
   "DOM.Iterable",
   "ESNext"
  ],
  "allowJs": false,
  "skipLibCheck": true,
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "strict": true,
  "forceConsistentCasingInFileNames": true,
  "module": "ESNext",
  "moduleResolution": "Node",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "noEmit": true,
  "jsx": "react-jsx",  
  "baseUrl": ".",
  "paths": {
    "@/*": ["./src/*"]
  }
 },
 "exclude": [
  "node_modules",
  "dist",
  "build"
 ],
 "include": [
  "src/**/*"
 ]
}